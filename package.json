{
  "name": "sdv-giftpicker",
  "version": "0.1.0",
  "private": true,
  "homepage": "https://Roang-zero1.github.io/sdv-giftpicker",
  "dependencies": {
    "ajv": "^6.5.1",
    "ajv-keywords": "^3.2.0",
    "bootstrap": "^4.1.1",
    "classnames": "^2.2.6",
    "immutability-helper": "^2.7.1",
    "npm-run-all": "^4.1.3",
    "prettier": "^1.13.5",
    "prop-types": "^15.6.2",
    "react": "^16.4.1",
    "react-app-rewire-hot-loader": "^1.0.1",
    "react-app-rewire-styled-components": "^3.0.2",
    "react-app-rewired": "^1.5.2",
    "react-dom": "^16.4.1",
    "react-hot-loader": "^4.3.2",
    "react-popper": "^1.0.0",
    "react-redux": "^5.0.7",
    "react-router-dom": "^4.3.1",
    "react-scripts-ts": "^2.16.0",
    "react-scroll": "^1.7.9",
    "react-transition-group": "^2.3.1",
    "reactstrap": "^6.1.0",
    "redux": "^4.0.0",
    "redux-persist": "^5.10.0",
    "styled-components": "^3.3.3",
    "styled-icons": "^2.2.0",
    "typesafe-actions": "^2.0.4"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!<rootDir>/node_modules/",
      "!<rootDir>/src/index.js",
      "!<rootDir>/src/registerServiceWorker.js",
      "!<rootDir>/src/data",
      "!<rootDir>/src/images"
    ],
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ]
  },
  "prettier": {
    "singleQuote": true
  },
  "scripts": {
    "start": "BROWSER=none react-app-rewired start --scripts-version react-scripts-ts",
    "build": "react-app-rewired build --scripts-version react-scripts-ts",
    "test": "react-scripts-ts test --env=jsdom --transformIgnorePatterns 'node_modules/(?!(styled-icons)/)'",
    "eject": "react-scripts-ts eject",
    "lint": "npm-run-all -p lint:*",
    "lint:css": "stylelint './src/**/*.{js,ts,jsx,tsx,css}'",
    "lint:ts": "tslint --project .",
    "precommit": "git-format-staged --formatter 'prettier --stdin' './src/**/*.{js,ts,jsx,tsx,css}' && lint-staged"
  },
  "lint-staged": {
    "./src/**/*.{js,ts,jsx,tsx}": [
      "stylelint",
      "tslint"
    ],
    "./src/**/*.css": [
      "stylelint"
    ]
  },
  "devDependencies": {
    "@types/classnames": "^2.2.4",
    "@types/enzyme": "^3.1.11",
    "@types/enzyme-adapter-react-16": "^1.0.2",
    "@types/jest": "^23.1.1",
    "@types/react": "^16.4.1",
    "@types/react-dom": "^16.0.6",
    "@types/react-redux": "^6.0.2",
    "@types/react-router-dom": "^4.2.7",
    "@types/react-scroll": "^1.5.3",
    "@types/reactstrap": "^8.0.2",
    "@types/redux-mock-store": "^1.0.0",
    "awesome-typescript-loader": "^5.2.0",
    "babel-plugin-styled-components": "^1.5.1",
    "enzyme": "^3.6.0",
    "enzyme-adapter-react-16": "^1.4.0",
    "enzyme-to-json": "^3.3.4",
    "git-format-staged": "^2.0.0",
    "husky": "^0.14.3",
    "jest-styled-components": "^6.1.0",
    "lint-staged": "^7.2.0",
    "react-test-renderer": "^16.4.1",
    "redux-mock-store": "^1.5.3",
    "source-map-loader": "^0.2.3",
    "stylelint": "^9.3.0",
    "stylelint-config-prettier": "^4.0.0",
    "stylelint-config-recommended": "^2.1.0",
    "stylelint-config-styled-components": "^0.1.1",
    "stylelint-processor-styled-components": "^1.3.1",
    "ts-jest": "^22.4.6",
    "tslint": "^5.10.0",
    "typescript": "^2.9.2"
  }
}
